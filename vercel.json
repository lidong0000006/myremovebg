{
    "framework": "create-react-app",
    "buildCommand": "npm run build",
    "devCommand": "npm start",
    "outputDirectory": "build",
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "^/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "^/logo(.*)",
            "dest": "/logo$1"
        },
        {
            "src": "^/(.*)",
            "dest": "/index.html"
        }
    ]
}
